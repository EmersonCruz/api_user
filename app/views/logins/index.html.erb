<h1>Logins#index</h1>
<p>Find me in app/views/logins/index.html.erb</p>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@users) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :password %>
      <%= f.password_field :surname %>

      <%= f.submit "Login" %>#, class: "btn btn-primary"
    <% end %>
  </div>
</div>